    "# --- Step 1: Create a dictionary for student grades ---\n",
    "student_grades = {}\n",
    "\n",
    "# Add or update grades for at least 3 students\n",
    "for i in range(3):\n",
    "    name = input(f\"Enter student {i+1} name: \")\n",
    "    grade = input(f\"Enter grade for {name}: \")\n",
    "    student_grades[name] = grade\n",
    "\n",
    "print(\"\\n----- Student Grades -----\")\n",
    "for name, grade in student_grades.items():\n",
    "    print(f\"{name}: {grade}\")\n",
    "\n",
    "\n",
    "# --- Step 2: Create a set for hobbies ---\n",
    "hobbies = set()\n",
    "\n",
    "print(\"\\nEnter at least 3 hobbies:\")\n",
    "for i in range(3):\n",
    "    hobby = input(f\"Enter hobby {i+1}: \")\n",
    "    hobbies.add(hobby)\n",
    "\n",
    "print(\"\\nHobbies Set:\", hobbies)\n",
    "\n",
    "# --- Step 3: Convert the set into a frozenset ---\n",
    "hobbies_frozen = frozenset(hobbies)\n",
    "\n",
    "print(\"\\n----- Frozenset of Hobbies -----\")\n",
    "print(hobbies_frozen)\n"
   ]
  }
